@model semifinishAdmin.Models.siixsem_models_t

@{
    ViewBag.Title = "Delete";
}
<div class="window" style="width:600px;">
    <div class="window-title">BORRAR MODELO</div>
    <div class="window-content">

        <h3>Are you sure you want to delete this?</h3>
        <div>
            <hr />
            <dl class="dl-horizontal">
                <dd>@Html.DisplayFor(model => model.se_description)</dd>
                <hr />
                <dt>SERIAL 1:</dt>
                <dd>@Html.DisplayFor(model => model.se_serial_pn_1)</dd>

                <dt>SERIAL 2:</dt>
                <dd>@Html.DisplayFor(model => model.se_serial_pn_2)</dd>

                <dt>SERIAL 3:</dt>
                <dd>@Html.DisplayFor(model => model.se_serial_pn_3)</dd>

                <dt>SERIAL 4:</dt>
                <dd>@Html.DisplayFor(model => model.se_serial_pn_4)</dd>

                <dt>SERIAL 5:</dt>
                <dd>@Html.DisplayFor(model => model.se_serial_pn_5)</dd>

                <dt>FECHA DE CREACION:</dt>
                <dd>@Html.DisplayFor(model => model.se_created_date)</dd>

            </dl>
            <hr />
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-actions no-color">
                    <input type="submit" value="Delete" class="btn btn-danger" /> |
                    @Html.ActionLink("Back to List", "Index")
                </div>
            }
        </div>
    </div>
</div>